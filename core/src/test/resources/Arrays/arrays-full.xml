<arrays xmlns="urn:arrays" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<!-- arrays-full.xml and arrays-compact.xml are the same, only json versions differ -->
	<intElem>1</intElem>
	<stringElem>abcd</stringElem>
	<array1>
		<!-- array1 is a pure array, elem 'arrayElement' can be compacted away in json -->
		<arrayElement>
			<fielda>1a</fielda>
			<fieldb>1b</fieldb>
			<fieldc>1c</fieldc>
		</arrayElement>
		<arrayElement>
			<fielda>2a</fielda>
			<fieldb>2b</fieldb>
			<fieldc>2c</fieldc>
		</arrayElement>
	</array1>
	<struct>
		<!-- struct is not a pure array, cannot skip 'repeatedElem' -->
		<repeatedElem>
			<a>1</a>
		</repeatedElem>
		<repeatedElem>
			<a>2</a>
		</repeatedElem>
		<otherElem>x</otherElem>
	</struct>
</arrays>
