<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<experiment xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://frinexbuild.mpi.nl/frinex.xsd" appNameDisplay="Adaptive Vocabulary Assessment 2" appNameInternal="advocas2" backgroundColour="#FFFFFF" complementColour0="#EAC3A3" complementColour1="#9D7B5E" complementColour2="#C69E7C" complementColour3="#FFEDDE" complementColour4="#FFFDFB" defaultScale="1.0" isScalable="true" preserveLastState="true" primaryColour0="#628D8D" primaryColour1="#385E5E" primaryColour2="#4A7777" primaryColour3="#96ADAD" primaryColour4="#D5D8D8" rotatable="true" showMenuBar="false" textFontSize="17">
    <preventWindowClose featureText="Bij reload worden ingetypte data verwiderd"/>
    <metadata>
        <field controlledMessage="Input at least 3 letters." controlledRegex=".'{'3,'}'" postName="workerId" preventServerDuplicates="true" registrationField="Wat is je proefpersoonnummer?"/>
        <field controlledMessage="." controlledRegex=".*" postName="stimuli" preventServerDuplicates="true" registrationField="stimuli"/>
        <field controlledMessage="Input at least 3 letters." controlledRegex="['\\'S'\\'s]'{'3,'}'" postName="feedBack" preventServerDuplicates="true" registrationField=""/>
    </metadata>
    <presenter displayOrder="1" menuLabel="Menu" type="menu" self="Menu" title="Menu">
        <centrePage/>
        <addPadding/>
        <htmlText featureText="&lt;div style=&quot;width:100&#37;&quot; &gt; &lt;img src=&quot;static/logo-max.gif&quot; align=&quot;left&quot; /&gt; &lt;img src=&quot;static/logo-lil.png&quot; align=&quot;right&quot;/&gt; &lt;/div&gt;"/>
        <addPadding/>
        <addPadding/>
        <addPadding/>
        <addPadding/>
        <addPadding/>
        <addPadding/>
        <htmlText featureText="Welkom bij deze woordenschattest."/>
        <addPadding/>
        <addPadding/>
        <targetButton featureText="Naar het experiment" target="Edit_User" />
        <addPadding/>
        <addPadding/>
        <targetButton featureText="Naar de administratie pagina" target="Admin" />
    </presenter>
    <presenter displayOrder="2" menuLabel="Edit User" next="Accord" type="metadata" self="Edit_User" title="">
        <metadataField fieldName="workerId"/>
        <saveMetadataButton featureText="Volgende" sendData="true" networkErrorMessage="&amp;nbsp;">
            <onSuccess>   
                <autoNextPresenter/>
            </onSuccess>
            <onError>   
                <autoNextPresenter/>
            </onError>      
        </saveMetadataButton>
    </presenter>
