    <presenter displayOrder="3" next="stimuliScreenV1" type="metadata" self="Accord" title="" >
        <centrePage/>
        <htmlText featureText="&lt;b&gt;&lt;big&gt;Instructies &lt;/big&gt; &lt;/b&gt;"/>
        <addPadding/>
        <addPadding/>
        <htmlText featureText="In dit experiment krijg je een aantal woorden aangeboden.&lt;br/&gt;
Sommige zijn bestaande Nederlandse woorden en sommige zijn nepwoorden.&lt;br/&gt;
Geef voor elk woord met een muisklik aan of jij het woord kent of niet."/>
        <addPadding/>
        <addPadding/>
        <htmlText featureText="&lt;b&gt;&lt;big&gt;Voorbeeld:&lt;/big&gt;&lt;/b&gt;"/>
        <addPadding/>
        <htmlText featureText="&lt;img src=&quot;static/voorbeeld-kleiner.png&quot; style=&quot;border: DarkGray;border-width: 1; border-style: solid;&quot; /&gt;"/>
        <addPadding/>
        <addPadding/>
        <htmlText featureText="&lt;b&gt;&lt;big&gt;Veel succes!&lt;/big&gt;&lt;/b&gt;"/>
        <addPadding/>
        <addPadding/>
        <targetButton featureText="Begin experiment" target="stimuliScreenV1"/>
    </presenter>
    <presenter displayOrder="4" type="stimulus" self="stimuliScreenV1" title="Series 1">
        <loadStimulus class="nl.mpi.tg.eg.frinex.adaptivevocabularyassessment.client.service.AdVocAsStimuliProvider" eventTag="stimuliN" 
                      numberOfBands="54" wordsPerBand="20" startBand="20" averageNonWordPosition="3" 
                      fineTuningTupleLength="4" nonwordsPerBlock="4" fineTuningUpperBoundForCycles="2"
                      fastTrackPresent="true" fineTuningFirstWrongOut="true"
                      wordsSource="Words_NL_2rounds_1" 
                      nonwordsSource="NonWords_NL_2rounds_1">
            <hasMoreStimulus>
                <clearPage/>
                <centrePage/>
              
                <!--                <showStimulusProgress/>-->
                <table styleName="stimulusContainerTab">
                    <row>
                        <column styleName="stimulusContainerCol"> 
                            <stimulusLabel styleName="groupMessageLabel"/>
                            <addPadding/>
                            <stimulusRatingButton eventTier="1" ratingLabelLeft="">
                                <nextStimulus repeatIncorrect="false"/>
                            </stimulusRatingButton>
                        </column>
                    </row>
                </table>
            </hasMoreStimulus>
            <endOfStimulus>
                <centrePage/>
                <targetButton featureText="Heel goed gedaan. Nu komt de tweede ronde. Klik op de button om door te gaan. Veel Succes!" target="stimuliScreenV2"/>
                <addPadding/>
                <sendStimuliReport type="user_summary" separator=";" headerKey="row000000"/>
                <sendStimuliReport type="fast_track" separator=";" headerKey="row000000"/>
                <sendStimuliReport type="fine_tuning" separator=";" headerKey="row000000"/>
                <showStimuliReport/>
                <addPadding/>
                <targetButton featureText="Heel goed gedaan. Nu komt de tweede ronde. Klik op de button om door te gaan. Veel Succes!" target="stimuliScreenV2"/>
                <addPadding/>
            </endOfStimulus>
            <randomGrouping/>
            <stimuli/>
        </loadStimulus>
    </presenter>
    <presenter displayOrder="6" type="stimulus" self="stimuliScreenV2" title="Series 2">
        <loadStimulus class="nl.mpi.tg.eg.frinex.adaptivevocabularyassessment.client.service.AdVocAsStimuliProvider" eventTag="stimuliN"
                      numberOfBands="54" wordsPerBand="20" startBand="20" averageNonWordPosition="3" 
                      fineTuningTupleLength="4" nonwordsPerBlock="4" fineTuningUpperBoundForCycles="2"
                      fastTrackPresent="true" fineTuningFirstWrongOut="true" 
                      wordsSource="Words_NL_2rounds_2" 
                      nonwordsSource="NonWords_NL_2rounds_2">
            <hasMoreStimulus>
                <clearPage/>
                <centrePage/>
                <table styleName="stimulusContainerTab">
                    <row>
                        <column styleName="stimulusContainerCol"> 
                            <stimulusLabel styleName="groupMessageLabel"/>
                            <addPadding/>
                            <stimulusRatingButton eventTier="1" ratingLabelRight="" ratingLabelLeft="">
                                <nextStimulus repeatIncorrect="false"/>
                            </stimulusRatingButton>
                        </column>
                    </row>
                </table>
            </hasMoreStimulus>
            <endOfStimulus>
                <centrePage/>
                <htmlText featureText="Dit is het einde van deze taak. Dankjewel voor je deelname!"/>
                <addPadding/>
                <sendStimuliReport type="user_summary" separator=";" headerKey="row000000"/>
                <sendStimuliReport type="fast_track" separator=";" headerKey="row000000"/>
                <sendStimuliReport type="fine_tuning" separator=";" headerKey="row000000"/>
                <showStimuliReport/>
                <htmlText featureText="Dit is het einde van deze taak. Dankjewel voor je deelname!"/>
                <addPadding/>
                <targetButton featureText="Naar de administratie pagina" target="Admin"/>
                <addPadding/>
            </endOfStimulus>
            <randomGrouping/>
            <stimuli/>
        </loadStimulus>
    </presenter>
    <presenter displayOrder="7" type="transmission" self="Admin" title="Administratie" next="ListParticipants">
        <sendMetadata>
            <onSuccess> 
                <sendAllData>
                    <onSuccess>
                        <htmlText featureText="The participant's data are uploaded to the server"/>
                        <addPadding/>
                        <addPadding/>
                        <eraseUsersDataButton featureText="Remove the user's data from the browser" target="ListParticipants" />
                        <addPadding/>
                        <addPadding/>
                        <targetButton featureText="To the administration page" target="Admin" />
                        <addPadding/>
                        <addPadding/>
                        <createUserButton featureText="Opnieuw beginnen" target="Menu"/>
                    </onSuccess>
                    <onError>
                        <plainText featureText="Cannot connect to the server, please check your internet connection and/or upload data later. Do not clean the browser's cache."/>
                        <addPadding/>
                        <addPadding/>
                        <targetButton featureText="Retry" target="Admin"/>
                        <addPadding/>
                        <addPadding/>
                        <createUserButton featureText="Opnieuw beginnen" target="Menu"/>
                    </onError>
                </sendAllData>
            </onSuccess>
            <onError>   
                <plainText featureText="Cannot connect to the server, please check your internet connection and/or upload data later. Do not clean the browser's cache."/>
                <addPadding/>
                <addPadding/>
                <targetButton featureText="Retry" target="Admin"/>
                <addPadding/>
                <addPadding/>
                <createUserButton featureText="Opnieuw beginnen" target="Menu"/>
            </onError>      
        </sendMetadata>
    </presenter>
    <presenter back="Menu" displayOrder="8" menuLabel="Deelnemer lijst" type="metadata" self="ListParticipants" title="Kiez deelnemer">
        <htmlText featureText="The participants which are still in local browser memory:"/>
        <selectUserMenu/>
        <addPadding/>
        <addPadding/>
        <targetButton featureText="To the landing page" target="Menu"/> 
        <addPadding/>
        <addPadding/>
        <targetButton featureText="To the admin page" target="Admin"/>   
    </presenter>   
    <presenter back="Menu" displayOrder="20" menuLabel="about" type="debug" self="about" title="about">
        <versionData/>
        <eraseLocalStorageButton/>
        <localStorageData/>
    </presenter>   
    <stimuli> 
       <stimulus audioPath="" code="" identifier="example1" pauseMs="0" ratingLabels="stopped,continue" tag="set1">
       </stimulus>            
   </stimuli>
</experiment>
