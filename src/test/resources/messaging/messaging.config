#===============================================================================
# メッセージングサーバの設定
#===============================================================================

# リクエストスレッド数
threadCount=5

# キューマネージャの名称
queueManagerName=TEST

# 送信メッセージのデフォルト有効期間(msec)
defaultTimeToLive=120000

# 送信キューの論理名
sendQueueName=TEST.RESPONSE

# 受信監視キューの論理名
receiveQueueName=TEST.REQUEST

# 受信監視タイムアウト時間(msec)
readTimeout=5000

# MOM製品の標準エラー出力を使用するか否か
useProductSystemErrorOutput=true

# 業務アクション格納パッケージ
actionPackage=please.change.me.tutorial.ss11AC

# フレームワーク制御ヘッダファイル名
headerFileName=header

# コードの初期ロード設定
# 初期ロードは行わない。
loadCodeOnStartUp=false

# メッセージの初期ロード設定
# 初期ロードは行わない。
loadMessageOnStartUp=false

# 業務日付のデフォルト区分
businessDateDefaultSegment=02

# validation時に対象の値にnullを許可するか否か
validationConvertorAllowNullValue=true

# 開閉局制御判定を行う際に参照するテーブル
requestTable_serviceAvailability=REQUEST

#===============================================================================
# サーバプロセス管理設定
#===============================================================================
# バッチリクエストテーブル
requestTable=BATCH_REQUEST

# バッチリクエストIDカラム名
requestIdColumnName=REQUEST_ID
